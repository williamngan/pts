{
  "name": "pts",
  "version": "0.2.3",
  "description": "pts",
  "main": "./dist/pts.js",
  "types": "./dist/pts.d.ts",
  "scripts": {
    "start": "webpack --watch",
    "generate-typings": "tsc -d",
    "bundle-typings": "dts-bundle --name pts --outputAsModuleFolder true --main dist/files/**/*.d.ts --out ../pts.d.ts",
    "typings": "npm run generate-typings && npm run bundle-typings",
    "build": "webpack",
    "minify": "babel dist/pts.js --out-file dist/pts.min.js",
    "test": "mocha --opts mocha.opts",
    "test:auto": "mocha --opts mocha.opts --watch",
    "prepare": "npm run build && npm run typings && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/williamngan/pts.git"
  },
  "author": "William Ngan",
  "license": "Apache",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/mocha": "2.2.44",
    "@types/node": "^8.0.26",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-minify": "^0.2.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.2.0",
    "babili-webpack-plugin": "^0.1.2",
    "chai": "^3.5.0",
    "dts-bundle": "^0.7.3",
    "mocha": "^3.5.0",
    "mocha-loader": "^1.1.1",
    "mocha-typescript": "^1.1.7",
    "source-map": "^0.5.7",
    "ts-loader": "^2.3.4",
    "ts-node": "^3.3.0",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.4.3",
    "typedoc": "^0.5.10",
    "typescript": "^2.5.2",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.7.1"
  },
  "bugs": {
    "url": "https://github.com/williamngan/pts/issues"
  },
  "homepage": "https://github.com/williamngan/pts"
}
